<?xml version="1.0"?>
<ruleset name="Fluent Cart PHP Compatibility">
    <description>PHP Compatibility for Fluent Cart Plugin</description>

    <file>../app</file>

    <rule ref="PHPCompatibility">
        <properties>
            <property name="testVersion" value="7.4-8.4"/>
        </properties>
    </rule>

    <rule ref="PHPCompatibilityWP"/>

    <rule ref="PHPCompatibility.FunctionUse.ArgumentFunctionsReportCurrentValue">
        <severity>0</severity>
    </rule>
</ruleset>
