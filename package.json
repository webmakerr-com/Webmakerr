  "license": "ISC",
  "engines": {
    "node": ">=20.0.0"
  },
  "bugs": {
    "url": "https://github.com/wpmanageninja/fluent-cart/issues"
  },
  "homepage": "https://github.com/wpmanageninja/fluent-cart#readme",
  "scripts": {
    "dev": "node resources/dev/vite.js && npm run remove_ds && vite | grep -v 'warn - The `content` option'",
    "watch": "npm run dev",
    "build": "node resources/dev/vite.js --build && npm run remove_ds && vite build && [ \"$npm_config_zip\" = \"true\" ] && npm run pack || true",
    "build:zip": "npm run build && npm run pack",
    "pack": "node resources/dev/zip.js",
    "abspath": "node resources/dev/add-abspath-check.js",
    "addindex": "node resources/dev/add-index-files.js",
    "checkabs": "node resources/dev/absissue.js",
    "zip": "node resources/dev/zip.js",
    "translate": "node resources/dev/translator.js",
    "translate-frontend": "node resources/dev/translator-frontend.js",
    "translate:issue": "node resources/dev/translation-issue.js",
    "translate:php": "node resources/dev/translation-issue-php.js",
    "remove_ds": "find . -name .DS_Store -delete",
    "production": "npm run build",
    "prod": "npm run build",
    "el_plus": "node resources/dev/element-plus-resolver.js",
    "el_plus_customer_profile": "node resources/dev/element-plus-resolver_customer.js",
    "translate:all": "npm run translate-frontend -- --payment %npm_config_argv% && npm run translate -- --customer %npm_config_argv% && npm run translate-frontend -- %npm_config_argv% && npm run translate -- %npm_config_argv% && npm run translate -- --block %npm_config_argv% && npm run translate --customer %npm_config_argv% && npm run translate --customer %npm_config_argv% && npm run translate --block %npm_config_argv%",
    "package-src": "node resources/dev/package-src.js"

  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@wordpress/api-fetch": "^6.55.0",
    "@wordpress/hooks": "^3.58.0",
    "@wordpress/url": "^4.9.0",
    "browserslist": "^4.24.4",
    "chart.js": "^4.4.5",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "element-plus": "^2.9.7",
    "flag-icons": "^7.2.3",
    "fuse.js": "^7.0.0",
    "google-palette": "^1.1.1",
    "html2canvas": "^1.4.1",
    "htmlparser2": "^9.1.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-dropdown-tree-select": "^2.8.0",
    "react-selectrix": "1.0.17",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.0",
    "vue-confetti-explosion": "^1.0.2",
    "vue-dd": "^1.6.7",
    "vue-draggable-next": "^2.2.1",
    "vue-easy-lightbox": "^1.17.0",
    "vue-router": "^4.4.5",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.25.7",
    "@tailwindcss/container-queries": "^0.1.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/compiler-sfc": "^3.5.12",
    "archiver": "^7.0.1",
    "autoprefixer": "10.4.5",
    "cli-progress": "^3.12.0",
    "glob": "^11.0.0",
    "postcss": "^8.4.47",
    "postcss-import": "^15.1.0",
    "resolve-url-loader": "^5.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "sass": "^1.89.2",
    "tailwindcss": "^3.4.14",
    "unplugin-auto-import": "^0.11.5",
    "vite": "^6.3.5",
    "vite-plugin-live-reload": "^3.0.5",
    "vite-plugin-static-copy": "^2.3.0",
    "vue-loader": "^17.4.2"
  }
}
